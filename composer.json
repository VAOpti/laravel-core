{
  "name": "vision-aura/laravel-core",
  "type": "library",
  "description": "This is an extension of the Laravel framework and the core package to build Laravel applications from.",
  "license": "proprietary",
  "authors": [
    {
      "name": "Ewald van 't Veer",
      "email": "vaopti@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^10.28",
    "laravel/sanctum": "^3.3",
    "spatie/laravel-permission": "^6.2",
    "romanzipp/laravel-model-doc": "^2.1"
  },
  "autoload": {
    "files": [
      "src/helpers.php",
      "src/Models/helpers.php"
    ],
    "psr-4": {
      "VisionAura\\LaravelCore\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "VisionAura\\LaravelCore\\Providers\\CoreServiceProvider"
      ]
    }
  }
}
